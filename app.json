{
  "expo": {
    "name": "Hojita",
    "slug": "hojita",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "hojita",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a472a"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Hojita necesita acceso a la cámara para identificar plantas",
        "NSPhotoLibraryUsageDescription": "Hojita necesita acceso a tus fotos para identificar plantas"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a472a"
      },
      "package": "com.hojita.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Hojita necesita acceso a la cámara para identificar plantas"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Hojita necesita acceso a tus fotos para identificar plantas"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b1eeb2cd-65c9-4b51-a182-0f823df7fd89"
      }
    },
    "owner": "kirogatofeo"
  }
}
